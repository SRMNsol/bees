# app
debug: <%= @app[:debug] %>

# dbal
db.options:
  driver:   pdo_mysql
  dbname:   <%= @database[:database] %>
  host:     <%= @database[:host] %>
  user:     <%= @database[:username] %>
  password: <%= @database[:password] %>
  charset:  utf8

# orm
orm.em.options:
  result_cache:
    driver: memcache
    host: <%= @memcached[:host] %>
    port: <%= @memcached[:port] %>
